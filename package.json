{
  "name": "overlay-service",
  "version": "1.0.0",
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "build": "rm -rf build && tsc && babel --source-maps inline --out-dir build build && rm -rf build/test",
    "postinstall": "yarn build",
    "dev-start": "NODE_PATH=. NODE_ENV=development node build/src/run",
    "prod-start": "NODE_PATH=. NODE_ENV=production node build/src/run",
    "lint": "./node_modules/.bin/tslint --project .",
    "format": "yarn prettify",
    "prettify": "./node_modules/.bin/prettier --single-quote es7 --write --print-width 120 --tab-width 4 \"./{src,test}/**/*.{j,t}s\""
  },
  "pre-commit": [
    "lint",
    "format"
  ],
  "dependencies": {
    "@types/axios": "0.14.0",
    "@types/chai": "4.1.3",
    "@types/config": "0.0.34",
    "@types/consul": "0.23.32",
    "@types/express": "4.11.1",
    "@types/get-port": "4.0.0",
    "@types/lodash": "4.14.108",
    "@types/mocha": "5.2.0",
    "@types/pg": "7.4.8",
    "@types/puppeteer": "5.4.3",
    "@types/sinon": "5.0.1",
    "@types/supertest": "2.0.4",
    "@types/uuid": "3.4.3",
    "@types/winston": "2.3.9",
    "axios": "0.18.0",
    "babel-cli": "6.26.0",
    "babel-plugin-module-resolver": "3.1.1",
    "config": "1.30.0",
    "consul": "0.34.0",
    "express": "4.16.3",
    "jsonschema": "1.2.4",
    "lodash": "4.17.10",
    "pg": "7.4.3",
    "pngjs": "6.0.0",
    "puppeteer": "9.1.1",
    "source-map-support": "0.5.6",
    "typescript": "4.2.4",
    "uuid": "8.3.2",
    "winston": "2.4.2",
    "winston-daily-rotate-file": "3.1.4"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "get-port": "4.0.0",
    "mocha": "5.1.1",
    "pre-commit": "1.2.2",
    "prettier": "1.12.1",
    "proxyquire": "2.0.1",
    "sinon": "6.0.0",
    "supertest": "3.1.0",
    "ts-node": "6.0.3",
    "tsconfig-paths": "3.3.2",
    "tslint": "5.10.0",
    "update-json-file": "1.1.1"
  }
}
